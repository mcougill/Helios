<div class="container-fluid">
    <div class="container">
        <div class="row">

            {{#if rides}} {{#each rides}} {{#if uber}}
            <div class="col-sm-3">
                <a href="#">
                    <div class="imageBox">
                        <img src="./assets/images/uberLogo.jpg" class="img-responsive" alt="uberLogo">
                        <h3>{{type}}</h3>
                        <h4>{{estimate}}</h4>
                    </div>
                </a>
            </div>
            {{else}}

            <div class="col-sm-3">
                <a href="#">
                    <div class="imageBox">
                        <img src="./assets/images/lyftLogo.jpg" class="img-responsive" alt="uberLogo">
                        <h3>{{type}}</h3>
                        <h4>{{estimate}}</h4>
                    </div>
                </a>
            </div>

            {{/if}} {{/each}} {{/if}}

        </div>
    </div>
</div>