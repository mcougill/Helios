 {{#if rides}} {{#each rides}} {{#if uber}}
<div class="container-fluid">
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                <a href="#">
                    <div class="imageBox">
                        <img src="./images/uberLogo.jpg" class="img-responsive" alt="uberLogo">
                        <h3>{{rides.type}}</h3>
                        <h4>{{rides.estimate}}</h4>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>
{{else}}
<div class="container-fluid">
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                <a href="#">
                    <div class="imageBox">
                        <img src="./images/lyftLogo.jpg" class="img-responsive" alt="uberLogo">
                        <h3>{{rides.type}}</h3>
                        <h4>{{rides.estimate}}</h4>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>
{{/if}} {{/each}} {{/if}}